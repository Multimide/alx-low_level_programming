# 1 "2-largest_number.c"
# 1 "main.h" 1
# 2 "2-largest_number.c" 2
# 11 "2-largest_number.c"
int largest_number(int a, int b, int c)
{
 int largest;

 if (a > b && a > c)
 {
  largest = a;
 }
 else if (b > a && b > c)
 {
  largest = b;
 }
 else
 {
  largest = c;
 }

 return (largest);
}
